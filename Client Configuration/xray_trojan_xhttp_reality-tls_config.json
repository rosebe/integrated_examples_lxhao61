{
  "log": {},
  "inbounds": [
    {
      "port": "1080",
      "protocol": "socks",
      "settings": {
        "udp": true
      }
    },
    {
      "port": "1081",
      "protocol": "http",
      "settings": {}
    }
  ],
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "domain": ["geosite:cn"], //中国大陆主流网站的域名
        "outboundTag": "direct" //与下 outbounds 中 tag 对应
      },
      {
        "type": "field",
        "ip": [
          "geoip:cn", //中国大陆的 IP
          "geoip:private" //私有地址 IP，如路由器等。
        ],
        "outboundTag": "direct" //与下 outbounds 中 tag 对应
      }
    ]
  },
  "outbounds": [
    {
      "protocol": "trojan",
      "settings": {
        "servers": [
          {
            "address": "2.2.2.2", //修改为使用本应用的 IPv4 对应域名或自己服务端的 IPv4
            "port": 443,
            "password": "diy2020" //修改为服务端配置的密码
          }
        ]
      },
      "streamSettings": {
        "network": "xhttp",
        "xhttpSettings": {
          "mode": "auto", //可根据实际情况修改来保证连通性
          "path": "/TSpLdG9k", //修改为服务端配置的路径
          "downloadSettings": {
            "address": "2.2.2.2", //修改为使用本应用的 IPv6 对应域名或自己服务端的 IPv6
            "port": 443,
            "network": "xhttp",
            "xhttpSettings": {
              "path": "/TSpLdG9k" //必须与上路径一致
            },
            "security": "tls",
            "tlsSettings": {
              "serverName": "h3.xx.yy", //修改为使用本应用的域名。（对应 address 使用 IP 后必须配置，否则可删除。）
              "alpn": ["h2"], //若服务端支持 HTTP/3 传输其值可改为 h3
              "fingerprint": "chrome" //模拟 TLS 指纹，可任一 chrome、firefox、safari、ios、edge、qq、random、randomized 选项配置。
            }
          }
        },
        "security": "reality",
        "realitySettings": {
          "serverName": "h3.xx.yy", //修改为服务端配置的域名
          "publicKey": "SU8ZzrdS1DETJj0y3u4xFkPTCcf6HwJCBbbjWCoMMHw", //修改为服务端私钥的对应公钥
          "shortId": "", //修改为服务端 shortIds 配置之一
          "spiderX": "", //选填，爬虫初始路径与参数，建议每个客户端不同。
          "fingerprint": "chrome" //模拟 TLS 指纹，可任一 chrome、firefox、safari、ios、edge、qq、random、randomized 选项配置。
        }
      }
    },
    {
      "tag": "direct",
      "protocol": "freedom",
      "settings": {}
    }
  ]
}
//备注：
//1、IPv4/IPv6 对应上行/下行可根据实际调整。
//2、两应用对应上行/下行可根据实际调整。
